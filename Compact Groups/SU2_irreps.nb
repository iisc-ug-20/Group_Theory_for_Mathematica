Cell[BoxData[
 RowBox[{
  RowBox[{"Irrep", "[", "l_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      SubscriptBox["L", "p"], "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"j", "==", 
           RowBox[{"i", "+", "1"}]}], ",", " ", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"l", 
             RowBox[{"(", 
              RowBox[{"l", "+", "1"}], ")"}]}], "-", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"l", "-", "i"}], ")"}], 
             RowBox[{"(", 
              RowBox[{"l", "+", "1", "-", "i"}], ")"}]}]}], "]"}], ",", " ", 
          "0"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "1", ",", " ", "n"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"j", ",", " ", "1", ",", " ", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["L", "m"], "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"j", "==", 
           RowBox[{"i", "-", "1"}]}], ",", " ", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"l", 
             RowBox[{"(", 
              RowBox[{"l", "+", "1"}], ")"}]}], "-", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"l", "+", "2", "-", "i"}], ")"}], 
             RowBox[{"(", 
              RowBox[{"l", "+", "1", "-", "i"}], ")"}]}]}], "]"}], ",", " ", 
          "0"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "1", ",", " ", "n"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"j", ",", " ", "1", ",", " ", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["L", "1"], "=", 
      FractionBox[
       RowBox[{
        SubscriptBox["L", "p"], "+", 
        SubscriptBox["L", "m"]}], "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["L", "2"], "=", 
      FractionBox[
       RowBox[{
        SubscriptBox["L", "p"], "-", 
        SubscriptBox["L", "m"]}], 
       RowBox[{"2", "I"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["L", "3"], "=", 
      RowBox[{"DiagonalMatrix", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"l", "+", "1", "-", "i"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", " ", "n"}], "}"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       SubscriptBox["L", "i"], ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "3"}], "}"}]}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.903595491414804*^9, 3.903595604401622*^9}, {
