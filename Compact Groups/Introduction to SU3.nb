        RowBox[{"[", "i", "]"}], "]"}], ",", 
       RowBox[{"FinalNames", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}], "]"}], "\[Equal]", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"a", "[", 
         RowBox[{"i", ",", "j", ",", "k"}], "]"}], 
        RowBox[{"MatRep", "[", 
         RowBox[{"FinalNames", "[", 
          RowBox[{"[", "k", "]"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", "8"}], "}"}]}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "1"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "2"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "3"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "4"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "5"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "6"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "7"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "8"}], "]"}]}], "}"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DecompElem", "[", 
