Cell[BoxData[{
 RowBox[{
  RowBox[{"Coeff", "[", 
   RowBox[{"i_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Commutator", "[", 
      RowBox[{
