Cell[TextData[{
 "Defining killing form enables us to make a connection between the Cartan \
subalgebra, H and its dual ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", "*"], TraditionalForm]],ExpressionUUID->
  "e2e438a3-211d-4467-a6fd-3043b4711260"],
 " (though this holds only for Semi-simple Lie Algebra that we are mostly \
concerned with). If \[Rho] ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association[
    "boxes" -> FormBox["\[Element]", TraditionalForm], "errors" -> {}, 
     "input" -> "\\in", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "c034266f-b0c1-42f8-ad3e-f96fad568a0e"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["H", "*"], TraditionalForm]],ExpressionUUID->
  "1faeb04f-c27a-4b3e-bc32-05bdeb8da833"],
 ", there exists a unique element ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "\[Rho]"], 
    TemplateBox[Association[
     "boxes" -> FormBox["\[Element]", TraditionalForm], "errors" -> {}, 
      "input" -> "\\in", "state" -> "Boxes"],
     "TeXAssistantTemplate"], " ", "H"}], TraditionalForm]],ExpressionUUID->
  "f5fadec4-3f32-46a9-88f8-02b82e5804ac"],
 " such that for every k ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association[
    "boxes" -> FormBox["\[Element]", TraditionalForm], "errors" -> {}, 
     "input" -> "\\in", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "a1f195c2-acd8-4427-acab-381a1a22534c"],
 " H, \[Rho] (k) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["h", "\[Rho]"], ",", "k"}]}], TraditionalForm]],
  ExpressionUUID->"8bd60f00-1e2c-47c8-a679-bc5ed69329ba"],
 "). "
}], "Text",
