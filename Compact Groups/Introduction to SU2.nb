Cell["Building Adjoint Representation for SU2:", "Text",
 CellChangeTimes->{{3.9034593556285725`*^9, 
  3.903459366597497*^9}},ExpressionUUID->"07714388-3034-4c9b-8aef-\
c4be9fea7770"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Coeff", "[", 
   RowBox[{"i_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Commutator", "[", 
      RowBox[{
       RowBox[{"ElementName", "[", 
        RowBox[{"[", "i", "]"}], "]"}], ",", 
       RowBox[{"ElementName", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}], "]"}], "\[Equal]", 
     RowBox[{"Sum", "[", "  ", 
      RowBox[{
       RowBox[{
        RowBox[{"a", "[", 
         RowBox[{"i", ",", "j", ",", "k"}], "]"}], 
        RowBox[{"MatRep", "[", 
         RowBox[{"ElementName", "[", 
          RowBox[{"[", "k", "]"}], "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", "3"}], "}"}]}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "1"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "2"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"i", ",", "j", ",", "3"}], "]"}]}], "}"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ElemToIdx", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"x", " ", "/.", " ", 
   RowBox[{"Thread", "[", 
    RowBox[{"ElementName", " ", "->", " ", 
     RowBox[{"Range", "[", 
      RowBox[{"1", ",", " ", "3"}], "]"}]}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"StrCoeff", "[", 
   RowBox[{"i_", ",", " ", "j_", ",", " ", "k_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"a", "[", 
    RowBox[{"i", ",", " ", "j", ",", " ", "k"}], "]"}], " ", "/.", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Coeff", "[", 
      RowBox[{"i", ",", " ", "j"}], "]"}], " ", "//", " ", "Flatten"}], 
    ")"}]}]}], "\n", 
 RowBox[{
  RowBox[{"AdjRep", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"StrCoeff", "[", 
     RowBox[{
      RowBox[{"ElemToIdx", "[", "x", "]"}], ",", " ", "k", ",", " ", "j"}], 
     "]"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"j", ",", " ", "1", ",", " ", "3"}], "}"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"k", ",", " ", "1", ",", " ", "3"}], "}"}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.903459348326883*^9, 3.9034593531769776`*^9}, {
