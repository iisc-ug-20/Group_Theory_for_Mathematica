     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{"i", "^", "2"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"i", ",", " ", "#"}], "}"}]}], "]"}], "\[Equal]", "n"}], 
        " ", "&&", " ", 
        RowBox[{"MemberQ", "[", 
