Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       StyleBox["r", "TI"], TraditionalForm], "errors" -> {}, "input" -> "r", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ed59807a-dfc9-4dc5-9057-395a852b1a29"],
 " be the number of  irreps (that is, the length of each of the tuples in ",
 StyleBox["PossibleDimSets", "Input"],
 "). And ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       StyleBox["s", "TI"], TraditionalForm], "errors" -> {}, "input" -> "s", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "804f1c43-c87d-4cd3-baf5-f5ce70933a64"],
 " be the number of conjugacy classes. Then there are ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         RowBox[{"(", 
           RowBox[{
             StyleBox["r", "TI"], "-", "1"}], ")"}], "(", 
         RowBox[{
           StyleBox["s", "TI"], "-", "1"}], ")"}], TraditionalForm], 
     "errors" -> {}, "input" -> "(r-1)(s-1)", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "2499c25a-f9b8-48ea-8026-1cce15163ca7"],
 " variables to solve for in order to find the character table, and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       FractionBox[
        RowBox[{
          RowBox[{"(", 
            RowBox[{
              StyleBox["r", "TI"], "-", "1"}], ")"}], "(", 
          RowBox[{
            StyleBox["r", "TI"], "+", "2"}], ")"}], "2"], TraditionalForm], 
     "errors" -> {}, "input" -> "\\frac{(r-1)(r+2)}{2}", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5c1bad74-2129-4ab2-b5a2-4a4e535306db"],
 " equations to solve. Additionally, for all irreps with dimension 1, the \
trace of each class must satisfy the property that trace raised to the power \
order equals 1, since the identity matrix in 1D has trace 1. \n\nThe \
equations that we solve are:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         UnderscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["g", "TI"], "\[Element]", 
            StyleBox["G", "TI"]}], LimitsPositioning -> True], 
         SuperscriptBox["\[Chi]", "\[CapitalGamma]"], 
         RowBox[{"(", 
           StyleBox["g", "TI"], ")"}], "\[NonBreakingSpace]", ".", 
         "\[NonBreakingSpace]", 
         OverscriptBox[
          RowBox[{
            SuperscriptBox["\[Chi]", "\[CapitalTheta]"], "(", 
            StyleBox["g", "TI"], ")"}], "_"], "\[LongEqual]", "|", 
         StyleBox["G", "TI"], "|", 
         SubscriptBox["\[Delta]", "\[CapitalGamma]\[CapitalTheta]"]}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\sum_{g\\in G} \
\\chi^{\\Gamma}(g)~.~\\bar{\\chi^{\\Theta}(g)}=|G|\\delta_{\\Gamma\\Theta}", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d226e02e-879e-4336-b7f7-338996dd15de"],
 "  \t\t\t\t\t\t\t\t\t\t...(1)"
}], "Text",
 CellChangeTimes->{
  3.8982156152893925`*^9, {3.898215726868676*^9, 3.8982157387929792`*^9}, {
   3.898215822552788*^9, 3.898215826128503*^9}, {3.89821607948547*^9, 
   3.8982160858119197`*^9}, {3.8982163178619833`*^9, 
   3.8982163185573845`*^9}},ExpressionUUID->"2806d306-6e85-4233-b075-\
a765ac9096e4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Function", " ", "that", " ", "solves", " ", "the", " ", "equations", " ", 
    "obtained", " ", "from", " ", 
    RowBox[{"Schur", "'"}], "s", " ", "lemma", " ", "and", " ", "returns", 
    " ", "the", " ", "character", " ", 
    RowBox[{"table", ".", " ", "The"}], " ", "index", " ", "is", " ", "of", 
    " ", "the", " ", "particular", " ", "tuple", " ", "that", " ", "we", " ", 
    "want", " ", "to", " ", "test", " ", 
    RowBox[{"out", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ProduceCharTable", "[", "index_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"xArr", "=", 
        RowBox[{"Array", "[", 
         RowBox[{"x", ",", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
            "rListDir", "\[LeftDoubleBracket]", "index", 
             "\[RightDoubleBracket]"}], ",", " ", "s"}], "}"}]}], "]"}]}], 
       ",", " ", "char", ",", " ", "equations", ",", " ", "knowns1", ",", " ",
        "knowns2", ",", " ", "knowns3", ",", " ", "unitDim"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"unitDim", "=", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{
          "rListDir", "\[LeftDoubleBracket]", "index", 
           "\[RightDoubleBracket]"}], "]"}], ",", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"DimSetsDir", "\[LeftDoubleBracket]", 
            RowBox[{"index", ",", " ", "#"}], "\[RightDoubleBracket]"}], 
           "\[Equal]", "1"}], "&"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      " ", 
      RowBox[{"equations", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"x", "[", 
              RowBox[{"i1", ",", "j"}], "]"}], "*", 
             RowBox[{"x", "[", 
              RowBox[{"i2", ",", " ", "j"}], "]"}], "*", 
             RowBox[{
             "ConjMultiplicities", "\[LeftDoubleBracket]", "j", 
              "\[RightDoubleBracket]"}]}], ",", " ", 
            RowBox[{"{", 
             RowBox[{"j", ",", " ", "1", ",", " ", "s"}], "}"}]}], "]"}], 
          "\[Equal]", 
          RowBox[{"n", "*", 
           RowBox[{"KroneckerDelta", "[", 
            RowBox[{"i1", ",", " ", "i2"}], "]"}]}]}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i1", ",", " ", "1", ",", " ", 
           RowBox[{
           "rListDir", "\[LeftDoubleBracket]", "index", 
            "\[RightDoubleBracket]"}]}], "}"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i2", ",", "1", ",", " ", 
           RowBox[{
           "rListDir", "\[LeftDoubleBracket]", "index", 
            "\[RightDoubleBracket]"}]}], "}"}]}], " ", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"knowns1", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{
         RowBox[{
         "xArr", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "==",
          "1"}], "]"}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{
       "All", " ", "matrices", " ", "in", " ", "trivial", " ", "irrep", " ", 
        "have", " ", "trace", " ", "1"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"knowns2", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Transpose", "[", "xArr", "]"}], "\[LeftDoubleBracket]", 
          "1", "\[RightDoubleBracket]"}], "\[Equal]", 
         RowBox[{
         "DimSetsDir", "\[LeftDoubleBracket]", "index", 
          "\[RightDoubleBracket]"}]}], "]"}]}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"In", " ", "all", " ", "irreps"}], ",", " ", 
        RowBox[{
         RowBox[{"identity", " ", "matrix", " ", "has", " ", "trace"}], " ", 
         "=", " ", "dimension"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"knowns3", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"xArr", "\[LeftDoubleBracket]", " ", 
             RowBox[{"unitDim", ",", " ", "t"}], "\[RightDoubleBracket]"}], 
            "^", 
            RowBox[{
            "Orders", "\[LeftDoubleBracket]", "t", 
             "\[RightDoubleBracket]"}]}], "\[Equal]", 
           RowBox[{"Table", "[", 
            RowBox[{"1", ",", " ", 
             RowBox[{"{", 
              RowBox[{"m", ",", "unitDim"}], "}"}]}], "]"}]}], " ", ",", " ", 
          RowBox[{"{", 
           RowBox[{"t", ",", " ", "1", ",", " ", "s"}], "}"}]}], "]"}], 
        "]"}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"In", " ", "every", " ", "1", "D", " ", "irrep"}], ",", " ", 
        RowBox[{
         RowBox[{"trace", "^", "order"}], " ", "must", " ", "be", " ", 
         "equal", " ", "to", " ", "1"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"char", "=", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"Join", "[", 
          RowBox[{
           RowBox[{"Flatten", "[", "equations", "]"}], ",", " ", "knowns1", 
           ",", " ", "knowns2", ",", " ", "knowns3"}], "]"}], ",", 
         RowBox[{"Flatten", "[", "xArr", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"char", "\[Equal]", 
         RowBox[{"{", "}"}]}], ",", " ", "char", ",", " ", 
        RowBox[{"xArr", "/.", "char"}]}], "]"}]}]}], "]"}]}]}]], "Input",
