   3.8979057052256823`*^9, 3.8979092182193856`*^9, 3.898206794021624*^9, 
   3.899228065731608*^9, 3.9011605607428527`*^9, 3.901256105580296*^9, 
   3.90125864940129*^9, 3.9016823516401033`*^9, 3.9016823951075*^9, 
   3.9019491352284803`*^9, {3.9019782640563307`*^9, 3.901978273645808*^9}, {
