Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Function", " ", "that", " ", "solves", " ", "the", " ", "equations", " ", 
    "obtained", " ", "from", " ", 
    RowBox[{"Schur", "'"}], "s", " ", "lemma", " ", "and", " ", "returns", 
    " ", "the", " ", "character", " ", 
    RowBox[{"table", ".", " ", "The"}], " ", "index", " ", "is", " ", "of", 
    " ", "the", " ", "particular", " ", "tuple", " ", "that", " ", "we", " ", 
    "want", " ", "to", " ", "test", " ", 
    RowBox[{"out", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ProduceCharTable", "[", "index_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"xArr", "=", 
        RowBox[{"Array", "[", 
         RowBox[{"x", ",", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
            "rListDir", "\[LeftDoubleBracket]", "index", 
             "\[RightDoubleBracket]"}], ",", " ", "s"}], "}"}]}], "]"}]}], 
       ",", " ", "char", ",", " ", "equations", ",", " ", "knowns1", ",", " ",
        "knowns2", ",", " ", "knowns3", ",", " ", "unitDim"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"unitDim", "=", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{
          "rListDir", "\[LeftDoubleBracket]", "index", 
           "\[RightDoubleBracket]"}], "]"}], ",", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"DimSetsDir", "\[LeftDoubleBracket]", 
            RowBox[{"index", ",", " ", "#"}], "\[RightDoubleBracket]"}], 
           "\[Equal]", "1"}], "&"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      " ", 
      RowBox[{"equations", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Sum", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"x", "[", 
              RowBox[{"i1", ",", "j"}], "]"}], "*", 
             RowBox[{"Conjugate", "[", 
              RowBox[{"x", "[", 
               RowBox[{"i2", ",", " ", "j"}], "]"}], "]"}], "*", 
             RowBox[{
             "ConjMultiplicities", "\[LeftDoubleBracket]", "j", 
              "\[RightDoubleBracket]"}]}], ",", " ", 
            RowBox[{"{", 
             RowBox[{"j", ",", " ", "1", ",", " ", "s"}], "}"}]}], "]"}], 
          "\[Equal]", 
          RowBox[{"n", "*", 
           RowBox[{"KroneckerDelta", "[", 
            RowBox[{"i1", ",", " ", "i2"}], "]"}]}]}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i1", ",", " ", "1", ",", " ", 
           RowBox[{
           "rListDir", "\[LeftDoubleBracket]", "index", 
            "\[RightDoubleBracket]"}]}], "}"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i2", ",", "1", ",", " ", 
           RowBox[{
           "rListDir", "\[LeftDoubleBracket]", "index", 
            "\[RightDoubleBracket]"}]}], "}"}]}], " ", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"knowns1", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{
         RowBox[{
         "xArr", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "==",
          "1"}], "]"}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{
       "All", " ", "matrices", " ", "in", " ", "trivial", " ", "irrep", " ", 
        "have", " ", "trace", " ", "1"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"knowns2", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Transpose", "[", "xArr", "]"}], "\[LeftDoubleBracket]", 
          "1", "\[RightDoubleBracket]"}], "\[Equal]", 
         RowBox[{
         "DimSetsDir", "\[LeftDoubleBracket]", "index", 
          "\[RightDoubleBracket]"}]}], "]"}]}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"In", " ", "all", " ", "irreps"}], ",", " ", 
        RowBox[{
         RowBox[{"identity", " ", "matrix", " ", "has", " ", "trace"}], " ", 
         "=", " ", "dimension"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"knowns3", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"xArr", "\[LeftDoubleBracket]", " ", 
             RowBox[{"unitDim", ",", " ", "t"}], "\[RightDoubleBracket]"}], 
            "^", 
            RowBox[{
            "Orders", "\[LeftDoubleBracket]", "t", 
             "\[RightDoubleBracket]"}]}], "\[Equal]", 
           RowBox[{"Table", "[", 
            RowBox[{"1", ",", " ", 
             RowBox[{"{", 
              RowBox[{"m", ",", "unitDim"}], "}"}]}], "]"}]}], " ", ",", " ", 
          RowBox[{"{", 
           RowBox[{"t", ",", " ", "1", ",", " ", "s"}], "}"}]}], "]"}], 
        "]"}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"In", " ", "every", " ", "1", "D", " ", "irrep"}], ",", " ", 
        RowBox[{
         RowBox[{"trace", "^", "order"}], " ", "must", " ", "be", " ", 
         "equal", " ", "to", " ", "1"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"char", "=", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"Join", "[", 
          RowBox[{
           RowBox[{"Flatten", "[", "equations", "]"}], ",", " ", "knowns1", 
           ",", " ", "knowns2", ",", " ", "knowns3"}], "]"}], ",", 
         RowBox[{"Flatten", "[", "xArr", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"char", "\[Equal]", 
         RowBox[{"{", "}"}]}], ",", " ", "char", ",", " ", 
        RowBox[{"xArr", "/.", "char"}]}], "]"}]}]}], "]"}]}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
